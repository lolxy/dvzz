.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  background-color: #f5f5f5;
  padding: 0;
  box-sizing: border-box;
  position: fixed;
} 
view, image, text, form,scroll-view{
  box-sizing: border-box;
}
image {
  max-width: 100%;
  max-height: 100%;
}