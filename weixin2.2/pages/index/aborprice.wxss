/* pages/index/aborprice.wxss */
@import '../../style/common.wxss';
.typelist {
  width: 100%;
  margin-bottom: 20rpx;
  padding: 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: #ffffff;
}
.typelist  view {
  font-size: 24rpx;
  line-height: 60rpx;
  color: #999999;
  border-bottom-width: 4rpx;
  border-bottom-style: solid;
  border-bottom-color: transparent;
}
.typelist  view.active {
  color: #1F80CD;
  border-bottom-color: #1F80CD;
}
.find_lst {
  width:100%;
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: stretch;
  background-color: #ffffff;
  border-bottom: 2rpx solid #ddd;
}
.find_item_pic {
  width: 140rpx;
  height: 140rpx;
  margin-right: 32rpx;
  border-radius: 10rpx;
}
.see_item {
  margin: 20rpx 0 45rpx;
  font-size: 30rpx;
  line-height: 1em;
  color: #333333;
}
.seee_item2 {
  width: 256rpx;
  font-size: 28rpx;
  line-height: 1em;
  color: #666666;
}
.seee_item2 span {
  font-size: 28rpx;
  line-height: 1em;
  color: #FF0000;
}